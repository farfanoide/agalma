<div class="row">
  <div class="small-12 large-4 columns">
    <ul class="items-container">
      <h2>Sucursales</h2>
      <% for branch in @branches %>
      <li><%= branch.name %></li>
      <% end %>
    </ul>
  </div>
  <div class="small-12 large-4 columns">
    <h2>Usuarios</h2>
    <ul class="items-container">
      <% for role in @roles %>
      <li><%= role.user.name %>(<%= role.role.name %>)</li>
      <% end %></ul>
    </div>
  </div>
